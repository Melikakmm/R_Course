#EX1

Ness <- setNames(c(7.45, 56, 39, 230, 132),
                      c('Volume', 'Area', 'Length',
                        'Max. depth', 'Min. depth'))

Lomond <- setNames(c(2.6, 71, 36, 190, 37),
                      c('Volume', 'Area', 'Length',
                        'Max. depth', 'Min. depth'))
Morar <- setNames(c(2.3, 27, 18.8, 310, 87),
                      c('Volume', 'Area', 'Length',
                        'Max. depth', 'Min. depth'))
Tay <- setNames(c(1.6, 26.4, 13, 150, 60.6),
                      c('Volume', 'Area', 'Length',
                        'Max. depth', 'Min. depth'))
Awe <- setNames(c(1.2, 39, 41, 94, 32),
                      c('Volume', 'Area', 'Length',
                        'Max. depth', 'Min. depth'))
Maree <- setNames(c(1.09, 28.6, 20, 114, 38),
                      c('Volume', 'Area', 'Length',
                        'Max. depth', 'Min. depth'))
Ericht <- setNames(c(1.08, 18.6, 23, 156, 57.6),
                      c('Volume', 'Area', 'Length',
                        'Max. depth', 'Min. depth'))
Lochy <- setNames(c(1.07, 16, 16, 162, 70),
                      c('Volume', 'Area', 'Length',
                        'Max. depth', 'Min. depth'))
Rannoch <- setNames(c(0.97, 19, 15.7, 134, 51),
                      c('Volume', 'Area', 'Length',
                        'Max. depth', 'Min. depth'))
Shiel <- setNames(c(0.79, 19.5, 28, 128, 40),
                      c('Volume', 'Area', 'Length',
                        'Max. depth', 'Min. depth'))
Katrine <- setNames(c(0.77, 12.4, 12.9, 151, 43.4),
                      c('Volume', 'Area', 'Length',
                        'Max. depth', 'Min. depth'))
Arkaig <- setNames(c(0.75, 16, 19.3, 109, 46.5),
                      c('Volume', 'Area', 'Length',
                        'Max. depth', 'Min. depth'))
Shin <- setNames(c(0.35, 22.5, 27.8, 49, 15.5),
                      c('Volume', 'Area', 'Length',
                        'Max. depth', 'Min. depth'))

Lakes<-c(Ness, Lomond, Morar, Tay, Awe, Maree, Ericht, Lochy, Rannoch, Shiel, Katrine,
          Arkaig, Shin)

scottish.lakes<-data.frame(Ness, Lomond, Morar, Tay, Awe, Maree, Ericht, Lochy, Rannoch, Shiel, Katrine,
                              Arkaig, Shin)

